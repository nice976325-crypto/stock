<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.HistoryinfoDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.HistoryinfoEntity" id="historyinfoMap">
        <result property="date" column="date"/>
        <result property="capitalinflow" column="capitalinflow"/>
        <result property="balance" column="balance"/>
        <result property="netbuying" column="netbuying"/>
        <result property="buybalance" column="buybalance"/>
        <result property="sellbalance" column="sellbalance"/>
        <result property="gainer" column="gainer"/>
        <result property="lzgztf" column="lzgztf"/>
        <result property="exponent" column="exponent"/>
        <result property="pricelimit" column="pricelimit"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.HistoryinfoVO" >
		SELECT * FROM historyinfo  historyinfo         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.HistoryinfoVO" >
		SELECT  historyinfo.* FROM historyinfo  historyinfo 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.HistoryinfoView" >

		SELECT  historyinfo.* FROM historyinfo  historyinfo 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.HistoryinfoView" >
		SELECT * FROM historyinfo  historyinfo <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
